package Resource.Recursion;

public class nthFibo{
	public static int fibo(int n) {
		if (n==1) {
			return 0;
		}
		if(n==2) {
			return 1;
		}
		return fibo(n-1)+fibo(n-2);
	}
	
	public static void main(String[]args) {
		for(int i=1;i<=5;i++) {
			if(i<=5) {
				System.out.println(nthFibo.fibo(5));
			}
			
			}
		}
	}
