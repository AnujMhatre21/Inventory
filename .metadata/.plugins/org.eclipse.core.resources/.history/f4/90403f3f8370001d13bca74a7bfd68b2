package Admin;

import java.util.LinkedHashMap;

public class TestAdmin {
	public static void main(String[] args) {

		String username = "ad";
		String pass = "adm";

		LinkedHashMap map1 = new LinkedHashMap();
		map1.put(username, pass);

		Service service = new Service();
		service.AllowAccess(map1);
	}

}
